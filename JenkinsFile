pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                sh 'javac HolaMundo.java'
            }
        }
        stage('Test') {
            steps {
                withMaven(maven : 'maven_3_3_9') {
                  sh 'mvn test'
              }
            }
        }
        stage('Deploy') {
            steps {
                sh 'cp HolaMundo /tmp/HolaMundo'
            }
        }
    }
}
