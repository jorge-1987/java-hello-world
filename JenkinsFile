pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                sh 'echo "Starting compile"'
                sh 'javac ./HolaMundo.java'
                sh 'echo "End compile"'
            }
        }
        stage('Test') {
            steps {
                  sh 'echo "test"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'cp ./HolaMundo /tmp/HolaMundo'
            }
        }
    }
}
