pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                sh 'javac HolaMundo.java'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                sh 'cp HolaMundo /tmp/HolaMundo'
            }
        }
    }
}
